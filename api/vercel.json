{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "server": "dist/server.js"
      }
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ]
}